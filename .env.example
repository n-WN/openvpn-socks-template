# Compose will read variables from a .env file placed next to compose.yml
# Copy this to .env and fill in values as needed.

# Exposes container's SOCKS5 port to host port
EXPOSE_SOCKS5_PORT=1080

# SOCKS5 settings inside container
SOCKS5_PORT=1080
SOCKS5_BIND=0.0.0.0
# Optional auth for SOCKS5
SOCKS5_USER=
SOCKS5_PASS=

# Optional: if your OpenVPN server requires username/password
OPENVPN_AUTH_USER=
OPENVPN_AUTH_PASS=
# Extra args passed to openvpn (space separated)
OPENVPN_EXTRA_ARGS=--verb 3 --auth-nocache
